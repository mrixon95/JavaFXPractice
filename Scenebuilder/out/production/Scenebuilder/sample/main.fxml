<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Circle?>

<AnchorPane fx:id="favouriteTitle" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Button layoutX="255.0" layoutY="52.0" mnemonicParsing="false" onAction="#up" text="Up" />
      <Button layoutX="491.0" layoutY="188.0" mnemonicParsing="false" onAction="#right" text="Right" />
      <Button layoutX="45.0" layoutY="182.0" mnemonicParsing="false" onAction="#left" text="Left" />
      <Button fx:id="titleL" layoutX="255.0" layoutY="328.0" mnemonicParsing="false" onAction="#down" text="Down" />
      <Circle fx:id="myCircle" fill="DODGERBLUE" layoutX="271.0" layoutY="200.0" radius="100.0" stroke="BLACK" strokeType="INSIDE" />
      <Label fx:id="favouriteLabel" layoutX="14.0" layoutY="21.0" prefHeight="17.0" prefWidth="532.0" text="My favourite Label" />
   </children>
</AnchorPane>
